{
	"compilerOptions": {
		"rootDir": "./src",
		"outDir": "./public",
		
		"target": "esnext",
		"module": "esnext",
		
		"noEmitOnError": true,
		"skipLibCheck": true,
		
		"strict": true,
		//"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true
	},
	"include": ["./src/**/*"]
}